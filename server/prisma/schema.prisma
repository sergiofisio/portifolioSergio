
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model portifolio {
  id          Int   @id @default(autoincrement())
  title       String
  description String
  image       String
  link        String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  url         String
  github      String
}